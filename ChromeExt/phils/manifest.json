{
  "manifest_version": 2,

  "name": "NuSys Importer",
  "description": "Makes it easy to bring content to NuSys",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
"web_accessible_resources": [
    "*.png",
    "*.html",
    "*.css",
    "*.svg"
],
"content_scripts" : [{
        "matches" : [ "<all_urls>"],
        "js" : ["keylistener.js"]
    }],

  "background": {
    "scripts": ["jquery.js", "bg.js"]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/",
      "http://*/",
        "https://*/"
  ]
}