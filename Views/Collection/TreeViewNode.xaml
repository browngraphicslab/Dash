<UserControl
    x:Class="Dash.Views.Collection.TreeViewNode"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Dash.Views.Collection"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300"
    d:DesignWidth="400"
    DataContextChanged="TreeViewNode_OnDataContextChanged"
    Tapped="TreeViewNode_OnTapped">

    <StackPanel>
        <TextBlock x:Name="XTextBlock" Foreground="White"></TextBlock>
        <local:CollectionTreeView x:Name="CollectionTreeView" Visibility="Collapsed"></local:CollectionTreeView>
    </StackPanel>
</UserControl>
