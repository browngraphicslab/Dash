<UserControl
    x:Class="Dash.Views.Document_Menu.DragOutDocIcon"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Dash.Views.Document_Menu"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="50"
    d:DesignWidth="50">
    <Grid Margin="0">
            <SymbolIcon Symbol="Go" HorizontalAlignment="Left" Margin="5,0,0,0" RenderTransformOrigin="0.5,0.5">
                <SymbolIcon.RenderTransform>
                    <CompositeTransform ScaleX="0.5" ScaleY="0.5"/>
                </SymbolIcon.RenderTransform>
            </SymbolIcon>

            <SymbolIcon Symbol="Page2" HorizontalAlignment="Left" Margin="0,10,0,0" RenderTransformOrigin="0.5,0.5">
                <SymbolIcon.RenderTransform>
                    <CompositeTransform ScaleX="0.5" ScaleY="0.5"/>
                </SymbolIcon.RenderTransform>
            </SymbolIcon>
        </Grid>
</UserControl>
