<UserControl
    x:Class="Dash.MenuButton"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="80"
    d:DesignWidth="70">
    <UserControl.Resources>
        <SolidColorBrush x:Key="MenuBackground" Color="{ThemeResource WindowsBlueColor}"></SolidColorBrush>
    </UserControl.Resources>
    <!--<Grid Width="55" Height="50">-->
    <StackPanel x:Name="xButtonStackPanel" Orientation="Vertical" ></StackPanel>
    <!--</Grid>-->
</UserControl>
